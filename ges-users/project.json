{
  "name": "ges-users",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "ges-users/src",
  "prefix": "workshop-event",
  "tags": [],
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/angular:ng-packagr-lite",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "ges-users/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "ges-users/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "ges-users/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "ges-users/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "ges-users/**/*.ts",
          "ges-users/**/*.html",
          "ges-users/package.json"
        ]
      }
    }
  }
}
